begin aerofoil
copy wing_mid
name fin_right
position_x     -1.41
position_y      3.3
position_z      0.42
offset_forward  0.04
offset_stalled  0
offset_reverse -0.04
chord           0.42
span            0.42
rotation       80
inc             3
CL_per_alpha    0.1
CL_0            0.0
CL_max          1.8
CL_min         -1.8
CL_per_deg      0.02
inc_per_deg     0.1
control_per_jx  0
control_per_jy  0
end aerofoil

begin aerofoil
copy fin_right
name fin_left
position_y 	-3.3
rotation	-80
